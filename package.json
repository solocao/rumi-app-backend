{
  "name": "rumi-server",
  "version": "1.0.0",
  "description": "API for apartment search",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon --watch './src/**/*.ts' --ignore './src/**/*.spec.ts' --exec 'ts-node --files' ./src/app.ts",
    "docker:push": "docker build -t knajjars/rumi-server . && docker push knajjars/rumi-server",
    "docker:push:arm": "docker buildx build --platform=linux/arm/v7 -t mourraille/rumi-armv7-1 .  && docker push mourraille/rumi-armv7-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knajjars/rumi-app-server.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.642.0",
    "bcrypt": "^3.0.8",
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.3",
    "mongoose": "^5.9.5",
    "mongoose-paginate-v2": "^1.3.6",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.15",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.45",
    "@types/mongoose": "^5.7.6",
    "@types/mongoose-paginate-v2": "^1.3.1",
    "@types/multer": "^1.4.2",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^12.12.30",
    "@types/node-fetch": "^2.5.5",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
